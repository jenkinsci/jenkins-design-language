{
    "name": "@jdl2/css",
    "version": "2.0.0-alpha.4",
    "scripts": {
        "prepublish": "npm run build",
        "build": "node-sass index.scss jdl.css && node-sass index.scss | uglifycss > jdl.min.css",
        "clean": "rm -f *.css"
    },
    "files": [
        "*.css",
        "**/*.scss"
    ],
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "node-sass": "^4.7.2",
        "uglifycss": "0.0.29"
    }
}
