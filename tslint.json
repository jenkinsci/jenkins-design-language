{
    "extends": ["tslint-config-airbnb", "tslint-config-prettier", "tslint-react-a11y"],
    "rules": {
        "no-increment-decrement": false,
        "no-this-assignment": false,
        "import-name": false,
        "function-name": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ]
    }
}
